local InfoTab     = Window:CreateTab("Info", 6031075938)

-- FPS + PING MONITOR
local RS = game:GetService("RunService")
local Stats = game:GetService("Stats")
local FPS = 0
getgenv().LiveFPS = 0
getgenv().LivePing = 0

RS.Heartbeat:Connect(function()
    FPS += 1
    if tick() % 1 < 0.02 then
        getgenv().LiveFPS = FPS
        FPS = 0
        pcall(function() getgenv().LivePing = math.floor(Stats.Network.ServerStatsItem["Data Ping"]:GetValue()) end)
    end
end)

local FPSLabel = MonitorTab:CreateLabel("FPS: Calculating...")
local PingLabel = MonitorTab:CreateLabel("Ping: Calculating...")
RS.RenderStepped:Connect(function()
    FPSLabel:Set("FPS: "..getgenv().LiveFPS)
    PingLabel:Set("Ping: "..getgenv().LivePing.." ms")
end)

-- ULTRA SAFE NUKE (NO CanCollide EVER = NO FALL)
local function SafeNuke()
    pcall(function()
        game.Lighting.GlobalShadows = false
        game.Lighting.FogEnd = 9e9
        settings().Rendering.QualityLevel = 1
        workspace.Terrain.WaterWaveSize = 0
        workspace.Terrain.WaterTransparency = 1
        if setfpscap then setfpscap(9999999999999) end
        if sethiddenproperty then
            sethiddenproperty(workspace, "StreamingMinRadius", 0)
            sethiddenproperty(workspace, "StreamingTargetRadius", 0)
        end
    end)
    for _,v in game:GetDescendants() do
        pcall(function()
            if v:IsA("Decal") or v:IsA("Texture") then v.Transparency = 1
            elseif v:IsA("ParticleEmitter") or v:IsA("Trail") or v:IsA("Smoke") or v:IsA("Fire") or v:IsA("Sparkles") or v:IsA("Beam") then v.Enabled = false
            elseif v:IsA("PostEffect") then v.Enabled = false
            elseif v:IsA("Sound") then v.Volume = 0 end
        end)
    end
end

-- V3 GOD MODE - 100% NO FALL (particles/decals only)
local V3Active = false
local V3Conn
local function StartV3()
    if V3Active then return end
    V3Active = true
    SafeNuke()
    V3Conn = RS.Heartbeat:Connect(function()
        pcall(function()
            for _,v in game:GetDescendants() do
                if v:IsA("ParticleEmitter") or v:IsA("Trail") or v:IsA("Smoke") or v:IsA("Fire") or v:IsA("Sparkles") or v:IsA("Beam") then
                    v.Enabled = false
                elseif v:IsA("Decal") or v:IsA("Texture") then
                    v.Transparency = 1
                end
            end
        end)
    end)
end
local function StopV3() if V3Conn then V3Conn:Disconnect() end V3Active = false end

-- NO FALL PROTECTION (extra safety)
local NoFallConn
MainTab:CreateToggle({Name="No Fall Protection (Immortal)", CurrentValue=true, Callback=function(v)
    if v then
        NoFallConn = RS.Stepped:Connect(function()
            local char = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()
            if char and char:FindFirstChild("HumanoidRootPart") and char.HumanoidRootPart.Position.Y < -100 then
                char.HumanoidRootPart.CFrame = CFrame.new(0, 100, 0)
            end
        end)
    else
        if NoFallConn then NoFallConn:Disconnect() end
    end
end})

-- BOOSTERS
MainTab:CreateToggle({Name="V1 Basic Boost", CurrentValue=true,  Callback=function(v) if v then SafeNuke() end end})
MainTab:CreateToggle({Name="V2 Advanced", CurrentValue=false, Callback=function(v) if v then SafeNuke() task.wait(0.2) SafeNuke() end end})
MainTab:CreateToggle({Name="V3 God Mode (NO FALL)", CurrentValue=false, Callback=function(v) if v then StartV3() else StopV3() end end})
MainTab:CreateToggle({Name="V4 (OP) Uncapped", CurrentValue=false, Callback=function(v) if v then SafeNuke() if setfpscap then setfpscap(1e13) end end end})

MainTab:CreateSlider({Name="FPS Cap", Range={60,10000000000000}, Increment=100000, CurrentValue=9999999999999, Callback=function(v) if setfpscap then setfpscap(v) end end})

