    Name = "FPS Cap (1–9999)",
    Range = {1, 9999},
    Increment = 10,
    CurrentValue = 9999,
    Callback = function(v)
        if setfpscap then setfpscap(v) end
        Rayfield:Notify({Title="FPS Cap", Content="Set to "..v.." FPS", Duration=3})
    end
})

