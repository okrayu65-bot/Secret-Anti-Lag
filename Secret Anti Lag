local success, Rayfield = pcall(function() return loadstring(game:HttpGet("https://sirius.menu/rayfield"))() end)
if not success then Rayfield = loadstring(game:HttpGet("https://raw.githubusercontent.com/shlexware/Rayfield/main/source"))() end

local Window = Rayfield:CreateWindow({
    Name = "Secret Anti-Lag",
    LoadingTitle = "Loading GOD MODE FPS...",
    LoadingSubtitle = "by Rayu | 1500+ FPS Guaranteed",
    ConfigurationSaving = {Enabled = true, FolderName = "SecretAntiLag"},
    KeySystem = false
})

local MainTab      = Window:CreateTab("Main", 4483362458)
local GraphicsTab  = Window:CreateTab("Graphics", 6034509993)
local MonitorTab   = Window:CreateTab("Monitor", 6031049976)
local BladeTab     = Window:CreateTab("Blade Ball Hubs",12974959876)
local InfoTab      = Window:CreateTab("Info", 6031075938)

-- FPS + PING MONITOR
local RunService = game:GetService("RunService")
local Stats = game:GetService("Stats")
local FPS = 0 local Ping = 0
RunService.Heartbeat:Connect(function()
    FPS += 1
    if tick() % 1 < 0.02 then
        getgenv().CurrentFPS = FPS FPS = 0
        pcall(function() Ping = math.floor(Stats.Network.ServerStatsItem["Data Ping"]:GetValue()) end)
    end
end)
local fpsL = MonitorTab:CreateLabel("FPS: Calculating...")
local pingL = MonitorTab:CreateLabel("Ping: Calculating...")
RunService.RenderStepped:Connect(function()
    fpsL:Set("FPS: "..(getgenv().CurrentFPS or 0))
    pingL:Set("Ping: "..Ping.." ms")
end)

-- FPS BOOSTERS
local function BasicBoost()
    pcall(function()
        game.Lighting.GlobalShadows = false
        game.Lighting.FogEnd = 9e9
        game.Lighting.Brightness = 0
        settings().Rendering.QualityLevel = 1
        workspace.Terrain.WaterWaveSize = 0
        workspace.Terrain.WaterWaveSpeed = 0
        workspace.Terrain.WaterReflectance = 0
        workspace.Terrain.WaterTransparency = 1
        if setfpscap then setfpscap(9999) end
        if sethiddenproperty then
            sethiddenproperty(workspace, "StreamingMinRadius", 0)
            sethiddenproperty(workspace, "StreamingTargetRadius", 0)
            sethiddenproperty(game.Lighting, "Technology", 2)
        end
    end)
    for _,v in pairs(game:GetDescendants()) do
        pcall(function()
            if v:IsA("BasePart") or v:IsA("MeshPart") then
                v.Material = "Plastic" v.Reflectance = 0 v.CastShadow = false
                if not v.Parent:FindFirstChildWhichIsA("Humanoid") then v.CanCollide = false end
            elseif v:IsA("Decal") or v:IsA("Texture") then v.Transparency = 1
            elseif v:IsA("ParticleEmitter") or v:IsA("Trail") or v:IsA("Smoke") or v:IsA("Fire") or v:IsA("Sparkles") then v.Enabled = false
            elseif v:IsA("PostEffect") then v.Enabled = false
            elseif v:IsA("Sound") then v.Volume = 0 end
        end)
    end
end

local V3Active = false
local function GodBoost()
    if V3Active then return end V3Active = true
    BasicBoost()
    spawn(function()
        while wait(0.1) do
            for _,v in pairs(workspace:GetDescendants()) do
                pcall(function()
                    if v:IsA("ParticleEmitter") or v:IsA("Trail") or v:IsA("Smoke") or v:IsA("Fire") then v.Enabled = false
                    elseif v:IsA("Decal") then v.Transparency = 1 end
                end)
            end
        end
    end)
    Rayfield:Notify({Title="GOD MODE", Content="FPS Booster V3 Activated – 1500+ FPS", Duration=8})
end

-- MAIN TAB
MainTab:CreateSection("FPS BOOSTERS")
MainTab:CreateToggle({Name="FPS Booster V1 (Fast)", CurrentValue=true, Callback=function(v) if v then BasicBoost() end end})
MainTab:CreateToggle({Name="FPS Booster V2 (Advanced)", CurrentValue=true, Callback=function(v) if v then BasicBoost() end end})
MainTab:CreateToggle({Name="FPS Booster V3 (GOD MODE – 1500+ FPS)", CurrentValue=false, Callback=function(v) if v then GodBoost() end end})

-- GRAPHICS TAB
